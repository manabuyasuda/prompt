extend /assets/pug/_layout
append variables
  - var pageTitle= site.name;
  - var pageDescription= site.description;
  - var pageKeywords= site.keywords;
  - var pageOgpTitle= pageTitle;
  - var pageOgpType= "website"; // トップページは"website"、それ以外は"article"を指定する
  - var pageOgpImage= site.ogpImage; // 共通画像を使わない場合は変更する
  - var pageOgpDescription= pageDescription; // `pageDescription`と同じものを使う

block content